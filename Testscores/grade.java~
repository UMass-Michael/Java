public class grade
{
  private String grade = new String();
  
  
    public void setAverage(String grade)
  {
    this.grade = grade;
  }
  
  public String letterGrade()
  {
    if (avg < 60)
    {
      grade = "F";
      return grade;
    }
    else
    {
      if (avg < 70)
      {
        grade = "D";
        return grade;
      }
      else
      {
        if (avg < 80)
        {
          grade = "C";
          return grade;
        }
        else
        {
          if (avg < 90)
          {
            grade = "B";
            return grade;
          }
          else
          {
            grade = "A";
            return grade;
          }
        }
      }
    }
  }
}